{
  "printWidth": 120,
  "semi": true,
  "tabWidth": 2,
  "useTabs": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "singleAttributePerLine": true,
  "overrides": [{ "files": "*.scss", "options": { "parser": "scss" } }]
}